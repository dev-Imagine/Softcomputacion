@{ 
    List<softcomputacion.Models.cliente> lstCliente = (List<softcomputacion.Models.cliente>)Session["Clientes"];
}
<div class="container-fluid">
    @foreach (var oCliente in lstCliente)
    {
        <div class="row m-t-8 p-b-8" style="border-bottom:1px solid #e9ecef">
            <div class="col-10 col-md-10">
                <p class="fs-16 m-t-8" id="pNombreApellido-@oCliente.idCliente">@oCliente.nombre @oCliente.apellido</p>
            </div>
            <div class="col-2 col-md-2 p-l-0 p-r-0">
                <button type="button" class="btn btn-outline-info" style="width:100%" onclick="SelecionarCliente(@oCliente.idCliente.ToString())"><i class="fas fa-check"></i></button>
            </div>
        </div>
    }
</div>




